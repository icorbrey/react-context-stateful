{
	"compilerOptions": {
		"strict": true,
		"target": "es5",
		"outDir": "dist",
		"baseUrl": "src",
		"sourceMap": true,
		"declaration": true,
		"module": "commonjs",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"dist",
		"node_modules"
	]
}
